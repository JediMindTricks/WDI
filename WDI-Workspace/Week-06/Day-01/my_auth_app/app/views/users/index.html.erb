<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<%= link_to "Sign Up", new_session_path %>

<% @user.each do |f| %>
	<ul>
		<li><%= f.name %></li>
	</ul>
<% end %>

<% if logged_in? %>
	<%= link_to "Logout", logout_path, {method: :delete} %>
<% else %>
	<%= link_to "Login", new_session_path %>
<% end %>